{
  "packageId": "<%= $CLI_PARAM_CustomPackageName %>.Data",
  "version": "<%= $CLI_PARAM_CustomPackageVersion %>",
  "description": "Cmf Custom <%= $CLI_PARAM_Tenant %> <%= $CLI_PARAM_CustomPackageName %>.Data Package",
  "packageType": "IoTData",
  "isInstallable": true,
  "isUniqueInstall": true,
  "contentToPack": [
    {
      "source": "$(version)/*",
      "target": "MasterData/$(version)",
      "contentType": "MasterData"
    },
    {
      "source": "AutomationWorkFlows/*",
      "target": "AutomationWorkFlows",
      "ignoreFiles": [
        "../.cmfpackageignore"
      ],
      "contentType": "AutomationWorkFlows"
    }
  ]
}