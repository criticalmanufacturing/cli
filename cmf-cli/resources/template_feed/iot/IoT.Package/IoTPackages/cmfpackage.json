{
  "packageId": "<%= $CLI_PARAM_CustomPackageName %>.Packages",
  "version": "<%= $CLI_PARAM_CustomPackageVersion %>",
  "description": "Cmf Custom <%= $CLI_PARAM_Tenant %> <%= $CLI_PARAM_CustomPackageName %>.Packages Package",
  "packageType": "IoT",
  "isInstallable": true,
  "isUniqueInstall": false,
  "contentToPack": [
    {
      "source": "src/*",
      "target": "node_modules",
      "ignoreFiles": [
        ".npmignore"
      ]
    }
  ],
  "dependencies": [
    {
      "id": "<%= $CLI_PARAM_CustomPackageName %>.Packages.Repository",
      "version": "<%= $CLI_PARAM_CustomPackageVersion %>"
    }
  ],
  "xmlInjection": [
    "ui.xml"
  ]
}
