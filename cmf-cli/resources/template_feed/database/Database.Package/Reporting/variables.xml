<variables>
    <variable name="Product.Database.Type" default="Microsoft SQL Server" valueType="Text" isRequired="true"/>
    
	<variable name="Package.ReportingServices.Address" valueType="HttpAddress" isRequired="true" />
	<variable name="Package.ReportingServices.Instance" valueType="Text" isRequired="false" />
	<variable name="Package.ReportingServices.PublicAddress" valueType="Url" isRequired="true" />
	<variable name="Package.ReportingServices.Username" valueType="Text" isRequired="true" />
	<variable name="Package.ReportingServices.Password" valueType="Password" isRequired="true" />
	
	<variable name="Package[Product.Database.Ods].Database.Server" valueType="Text" isRequired="true" />
    <variable name="Package[Product.Database.Ods].Database.User" valueType="Text" isRequired="true" />
    <variable name="Package[Product.Database.Ods].Database.Password" valueType="Password" isRequired="true" />

    <variable name="Package[Product.Database.Dwh].Database.Server" valueType="Text" isRequired="true" />
    <variable name="Package[Product.Database.Dwh].Database.User" valueType="Text" isRequired="true" />
    <variable name="Package[Product.Database.Dwh].Database.Password" valueType="Password" isRequired="true" />
	
	<variable name="Package[Product.Database.As].Database.Server" valueType="Text" isRequired="true" />
    <variable name="Package[Product.Database.As].Database.User" readOnly="false" isRequired="true" valueType="Text" label="Database User" placeholder="DBUser" />
    <variable name="Package[Product.Database.As].Database.Password" readOnly="false" isRequired="true" valueType="Password" label="Database User Password" placeholder="password" />

</variables>