{
  "name": "documentation-portal",
  "version": "<%= $CLI_PARAM_MESVersion %>",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "docs-shell": "node ./node_modules/cmf-docs-shell/scripts/index.js",
    "build-db": "npm run docs-shell build-db",
    "check-md-links": "npm run docs-shell check-md-links",
    "watch": "ng build --watch --configuration development",
    "extract-i18n": "ng extract-i18n --output-path ./src/assets/ --format json",
    "serve": "cross-env NODE_OPTIONS=--max-old-space-size=8192 npm run start -- --host 0.0.0.0 --disable-host-check --port 7001"
  },
  "private": true,
  "dependencies": {
    "@angular/core": "^<%= $CLI_PARAM_ngVersion %>",
    "@angular/router": "^<%= $CLI_PARAM_ngVersion %>",
    "@angular/service-worker": "^<%= $CLI_PARAM_ngVersion %>",
    "cmf-core": "<%= $CLI_PARAM_MESVersion-ReleaseTag %>",
    "cmf-core-business-controls": "<%= $CLI_PARAM_MESVersion-ReleaseTag %>",
    "cmf-core-controls": "<%= $CLI_PARAM_MESVersion-ReleaseTag %>",
    "cmf-core-shell": "<%= $CLI_PARAM_MESVersion-ReleaseTag %>",
    "cmf-docs-area-designsystem": "<%= $CLI_PARAM_MESVersion-ReleaseTag %>",
    "cmf-docs-controls": "<%= $CLI_PARAM_MESVersion-ReleaseTag %>",
    "cmf-docs-shell": "<%= $CLI_PARAM_MESVersion-ReleaseTag %>",
    "mermaid": "10.9.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^<%= $CLI_PARAM_ngVersion %>", 
    "@angular-eslint/schematics": "<%= $CLI_PARAM_ngVersion %>",
    "@angular/compiler-cli": "^<%= $CLI_PARAM_ngVersion %>",
    "@criticalmanufacturing/ngx-schematics": "<%= $CLI_PARAM_NGXSchematicsVersion %>",
    "cross-env": "^7.0.3"
  }
}
