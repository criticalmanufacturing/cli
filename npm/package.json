    "axios": "^1.7.9",
    "proxy-agent": "^5.0.0",
    "debug": "^4.3.4",
    "global-dirs": "3.0.0",
    "mkdirp": "^2.1.6",
    "node_modules-path": "^2.0.7",
    "rimraf": "^3.0.2",
    "tmp": "0.2.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "http-proxy-agent": "^7.0.2",
    "https-proxy-agent": "^7.0.5",
    "proxy-from-env": "^1.1.0"
